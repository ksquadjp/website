    "astro": "^3.0.0",
    "preact": "^10.13.2",
    "sharp": "^0.32.0",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.6",
    "cheerio": "1.0.0-rc.12",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.8.0",
