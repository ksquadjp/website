    "astro": "^3.0.12",
    "preact": "^10.17.1",
    "sharp": "^0.32.5",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.6.0",
    "cheerio": "1.0.0-rc.12",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-tailwindcss": "^0.5.4",
