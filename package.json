    "@astrojs/tailwind": "^2.1.3",
    "astro": "^1.9.2",
    "preact": "^10.11.3",
    "sharp": "^0.31.3",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "prettier": "^2.8.3",
    "prettier-plugin-astro": "^0.7.2",
